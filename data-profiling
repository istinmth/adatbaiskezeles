                "SELECT \n",
                "    MIN(oradij) AS 'Min',\n",
                "    MAX(oradij) AS 'Max',\n",
                "    AVG(oradij) AS 'Avg',\n",
                "    (SELECT oradij FROM \n",
                "        (SELECT oradij, ROW_NUMBER() OVER (ORDER BY oradij) AS rn, COUNT(*) OVER () AS cnt\n",
                "         FROM munka) sub\n",
                "     WHERE rn IN (FLOOR((cnt + 1) / 2.0), CEILING((cnt + 1) / 2.0))) AS Median,\n",
                "     STDEV(oradij) AS Stddev\n",
                "FROM munka"
            ],
