            "source": [
                "SELECT \n",
                "    count(oradij) AS 'Hanyfele',\n",
                "    (SELECT oradij FROM \n",
                "        (SELECT oradij, ROW_NUMBER() OVER (ORDER BY oradij) AS rn, COUNT(*) OVER () AS cnt\n",
                "         FROM munka) sub\n",
                "     WHERE rn = FLOOR((cnt + 1) / 4.0)) AS Q1,\n",
                "    (SELECT oradij FROM \n",
                "        (SELECT oradij, ROW_NUMBER() OVER (ORDER BY oradij) AS rn, COUNT(*) OVER () AS cnt\n",
                "         FROM munka) sub\n",
                "     WHERE rn = FLOOR((cnt + 1) * 3 / 4.0)) AS Q3,\n",
                "    ((SELECT oradij FROM \n",
                "        (SELECT oradij, ROW_NUMBER() OVER (ORDER BY oradij) AS rn, COUNT(*) OVER () AS cnt\n",
                "         FROM munka) sub\n",
                "     WHERE rn = FLOOR((cnt + 1) * 3 / 4.0))\n",
                "    -\n",
                "    (SELECT oradij FROM \n",
                "        (SELECT oradij, ROW_NUMBER() OVER (ORDER BY oradij) AS rn, COUNT(*) OVER () AS cnt\n",
                "         FROM munka) sub\n",
                "     WHERE rn = FLOOR((cnt + 1) / 4.0))) AS IQR\n",
                "FROM munka"
            ],
