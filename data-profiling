                "SELECT row_type AS 'Sortípus',\n",
                "    COUNT(*) AS 'Hány darab'\n",
                "FROM\n",
                "    (SELECT\n",
                "        CASE WHEN allas IS NULL THEN 'Null'\n",
                "            ELSE 'Nem üres' \n",
                "            END AS row_type\n",
                "    FROM munka) rows\n",
                "GROUP BY row_type"
