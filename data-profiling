                "SELECT MIN(LEN(allas)) AS szoveghossz_min, \n",
                "       MAX(LEN(allas)) AS szoveghossz_max, \n",
                "       AVG(LEN(allas)) AS szoveghossz_avg, \n",
                "       STDEV(LEN(allas)) AS szoveghossz_stddev, \n",
                "       VAR(LEN(allas)) AS szoveghossz_var\n",
                "FROM munka\n",
                "WHERE allas IS NOT NULL\n",
